{
  "devDependencies": {
    "@types/jest": "^23.1.1",
    "@types/node": "^10.3.5",
    "jest": "^23.2.0",
    "ts-jest": "^22.4.6",
    "ts-node": "^7.0.1",
    "tslint": "^5.10.0",
    "typescript": "^2.9.2"
  },
  "scripts": {
    "test": "jest"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "globals": {
      "ts-jest": {
        "tsConfigFile": "tsconfig.json"
      }
    },
    "testMatch": [
      "**/__tests__/*.+(ts|tsx|js)"
    ]
  },
  "dependencies": {
    "tslib": "^1.9.3"
  }
}
